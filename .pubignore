# .pubignore - Files to exclude from pub.dev publishing
# Similar to .gitignore but specifically for pub publishing

# Development files
.vscode/
.idea/
*.iml
.dart_tool/
.packages
.metadata
build/
.flutter-plugins
.flutter-plugins-dependencies

# Documentation website
website/

# Example app deployment files
example/build/
example/.dart_tool/
example/.packages
example/.flutter-plugins
example/.flutter-plugins-dependencies
example/web/
example/android/app/src/main/java/
example/android/gradle/
example/android/gradlew*
example/ios/Pods/
example/ios/Runner.xcworkspace/
example/ios/Runner.xcodeproj/project.xcworkspace/
example/macos/Pods/

# Git-generated files that shouldn't be published
example/lib/git_stamp/

# Cloudflare deployment files
example/wrangler.toml

# Development scripts and tools
scripts/
tools/

# Logs and temporary files
*.log
logs.txt
tmp/
temp/

# OS files
.DS_Store
Thumbs.db

# Coverage reports
coverage/

# Test files that shouldn't be published
test_driver/
integration_test/

# Local configuration files
.env*
local.properties

# IDE configurations
.vscode/
.idea/

# Package manager files
.pub-cache/
.pub/

# Analysis and build artifacts
.analysis_options_flutter.yaml
pids
*.pid
*.seed
*.pid.lock

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
*.lcov

# nyc test coverage
.nyc_output

# Dependency directories
node_modules/

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Microbundle cache
.rpt2_cache/
.rts2_cache_cjs/
.rts2_cache_es/
.rts2_cache_umd/

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# dotenv environment variables file
.env

# parcel-bundler cache (https://parceljs.org/)
.cache
.parcel-cache

# next.js build output
.next

# nuxt.js build output
.nuxt

# vuepress build output
.vuepress/dist

# Serverless directories
.serverless/

# FuseBox cache
.fusebox/

# DynamoDB Local files
.dynamodb/

# TernJS port file
.tern-port

# Stores VSCode versions used for testing VSCode extensions
.vscode-test